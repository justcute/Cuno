<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Trailer</th>
      <th>Date</th>
      <th>Duration</th>
      <th>Sertification</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.title %></td>
        <td><%= product.description %></td>
        <td><%= product.trailer %></td>
        <td><%= product.date %></td>
        <td><%= product.duration %></td>
        <td><%= product.sertification %></td>
        <td><%= product.country %></td>
        <td><%= link_to 'Show', product %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>